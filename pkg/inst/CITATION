         title = "probdistr: handbook on probability distributions",
         author = "Christophe Dutang et al.",
	 year = 2009,
         note = "R package version 1.00",

         textVersion =
	 paste(	"C. Dutang et al.",
		"(2009),",
	       	"probdistr: handbook on probability distributions.")
)

